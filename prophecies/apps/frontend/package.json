{
  "name": "frontend",
  "version": "0.5.18",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@icij/murmur": "3.11.7",
    "@vuex-orm/core": "^0.36.4",
    "@vuex-orm/plugin-axios": "^0.9.4",
    "axios": "^0.24.0",
    "bootstrap": "4.6.2",
    "bootstrap-vue": "^2.23.1",
    "core-js": "^3.34.0",
    "hotkeys-js": "^3.13.2",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "moment": "^2.29.1",
    "mutationobserver-shim": "^0.3.7",
    "sass": "^1.69.5",
    "vue": "^2.6.14",
    "vue-clickaway": "^2.2.2",
    "vue-feather-icons": "^5.1.0",
    "vue-i18n": "^8.27.0",
    "vue-multiselect": "^2.1.8",
    "vue-router": "^3.2.0",
    "vue-scrollto": "^2.20.0",
    "vue-shortkey": "^3.1.7",
    "vue-wait": "^1.5.3",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.1",
    "@icij/eslint-config": "^1.1.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-standard": "^9.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^29.2.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "msw": "^0.36.8",
    "sass-loader": "^16.0.4",
    "sass-resources-loader": "^2.2.3",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-tracker": "^3.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "@icij/eslint-config"
    ],
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "globalSetup": "<rootDir>/tests/unit/global-setup.js",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/setup.js"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "resolutions": {
    "@achrinza/node-ipc": "9.2.9"
  }
}
